    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
